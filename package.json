{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/tailwind.css",
    "dev": "concurrently \"pnpm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -o ./app/styles/app.css"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/vercel": "^1.7.2",
    "@vercel/node": "^2.5.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.0",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "postcss": "^8.4.16",
    "prisma": "^4.4.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
